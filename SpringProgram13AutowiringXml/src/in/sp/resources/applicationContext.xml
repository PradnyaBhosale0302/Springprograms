<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean class="in.sp.beans.Address" id="addrId1">
    	<property name="houseno" value="111"></property>
    	<property name="city" value="Bandra"></property>
    	<property name="pincode" value="123456"></property>
    </bean>
    <bean class="in.sp.beans.Address" id="addrId2" autowire-candidate="false">
    	<property name="houseno" value="222"></property>
    	<property name="city" value="Bandra"></property>
    	<property name="pincode" value="123456"></property>
    </bean>
    <bean class="in.sp.beans.Student" id="stdId" autowire="byType">
    	<property name="name" value="Pradnya"></property>
    	<property name="rollno" value="222"></property>
    	<!--  <property name="address" ref="addrId"></property> Manually DI-->
    </bean>

</beans>